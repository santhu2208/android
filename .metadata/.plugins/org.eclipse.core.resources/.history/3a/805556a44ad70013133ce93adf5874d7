package com.example.download.utils;

import java.io.InputStream;
import java.net.URL;

public class DownUtil {
	URL url =null;
	/*
	 * 从http地址  获取输入流
	 */
	public InputStream getIuputStream(String urlStr){
		
		try {
			url = new URL(urlStr);
			return url.openConnection().getInputStream();
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
		
	} 
}
